page {
  background-color: #f5f5f5;
  color: #333333;
  font-size: 28rpx;
  line-height: 1.5;
}
.container {
  padding: 24rpx;
}
.text-center {
  text-align: center;
}
.text-left {
  text-align: left;
}
.text-right {
  text-align: right;
}
.flex {
  display: flex;
}
.flex-1 {
  flex: 1;
}
.flex-center {
  display: flex;
  align-items: center;
  justify-content: center;
}
.flex-between {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.text-primary {
  color: #ff6b35;
}
.text-secondary {
  color: #666666;
}
.text-success {
  color: #4caf50;
}
.text-warning {
  color: #ff9800;
}
.text-error {
  color: #f44336;
}
.bg-primary {
  background-color: #ff6b35;
}
.bg-white {
  background-color: #ffffff;
}
.bg-gray {
  background-color: #f5f5f5;
}
.m-0 {
  margin: 0;
}
.m-1 {
  margin: 8rpx;
}
.m-2 {
  margin: 16rpx;
}
.m-3 {
  margin: 24rpx;
}
.m-4 {
  margin: 32rpx;
}
.p-0 {
  padding: 0;
}
.p-1 {
  padding: 8rpx;
}
.p-2 {
  padding: 16rpx;
}
.p-3 {
  padding: 24rpx;
}
.p-4 {
  padding: 32rpx;
}
.rounded-sm {
  border-radius: 8rpx;
}
.rounded-md {
  border-radius: 16rpx;
}
.rounded-lg {
  border-radius: 24rpx;
}
.rounded-xl {
  border-radius: 32rpx;
}
.rounded-full {
  border-radius: 50%;
}
.shadow-sm {
  box-shadow: 0 4rpx 8rpx rgba(0, 0, 0, 0.1);
}
.shadow-md {
  box-shadow: 0 8rpx 16rpx rgba(0, 0, 0, 0.1);
}
.shadow-lg {
  box-shadow: 0 16rpx 32rpx rgba(0, 0, 0, 0.15);
}
.gradient-primary {
  background: linear-gradient(135deg, #ff6b35 0%, #f093fb 100%);
}
.gradient-food {
  background: linear-gradient(135deg, #ff6b35 0%, #f093fb 50%, #f5f7fa 100%);
}
.gradient-life {
  background: linear-gradient(135deg, #2196f3 0%, #9c27b0 50%, #e91e63 100%);
}
.btn {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 16rpx 24rpx;
  border-radius: 16rpx;
  font-size: 28rpx;
  font-weight: 600;
  border: none;
  cursor: pointer;
  transition: all 0.3s ease;
}
.btn.btn-primary {
  background: #ff6b35;
  color: #ffffff;
}
.btn.btn-primary:hover {
  opacity: 0.9;
}
.btn.btn-secondary {
  background: #f5f5f5;
  color: #333333;
  border: 2rpx solid #eeeeee;
}
.btn.btn-secondary:hover {
  background: #eeeeee;
}
.btn.btn-outline {
  background: transparent;
  color: #ff6b35;
  border: 2rpx solid #ff6b35;
}
.btn.btn-outline:hover {
  background: #ff6b35;
  color: #ffffff;
}
.card {
  background: #ffffff;
  border-radius: 32rpx;
  box-shadow: 0 8rpx 16rpx rgba(0, 0, 0, 0.1);
  overflow: hidden;
  margin-bottom: 24rpx;
}
.card-header {
  padding: 24rpx;
  border-bottom: 2rpx solid #eeeeee;
}
.card-content {
  padding: 24rpx;
}
.card-footer {
  padding: 24rpx;
  border-top: 2rpx solid #eeeeee;
}
.form-item {
  margin-bottom: 24rpx;
}
.form-label {
  display: block;
  font-size: 28rpx;
  font-weight: 600;
  margin-bottom: 8rpx;
  color: #333333;
}
.form-input {
  width: 100%;
  height: 80rpx;
  padding: 0 24rpx;
  border: 2rpx solid #eeeeee;
  border-radius: 16rpx;
  font-size: 28rpx;
  background: #ffffff;
}
.form-input:focus {
  border-color: #ff6b35;
  outline: none;
}
.form-input::-webkit-input-placeholder {
  color: #999999;
}
.form-input::placeholder {
  color: #999999;
}
.form-textarea {
  width: 100%;
  min-height: 160rpx;
  padding: 24rpx;
  border: 2rpx solid #eeeeee;
  border-radius: 16rpx;
  font-size: 28rpx;
  line-height: 1.5;
  background: #ffffff;
  resize: vertical;
}
.form-textarea:focus {
  border-color: #ff6b35;
  outline: none;
}
.form-textarea::-webkit-input-placeholder {
  color: #999999;
}
.form-textarea::placeholder {
  color: #999999;
}
.tag {
  display: inline-block;
  padding: 8rpx 16rpx;
  background: rgba(255, 107, 53, 0.1);
  color: #ff6b35;
  border: 2rpx solid rgba(255, 107, 53, 0.2);
  border-radius: 8rpx;
  font-size: 24rpx;
  margin-right: 8rpx;
  margin-bottom: 8rpx;
}
.avatar {
  border-radius: 50%;
  overflow: hidden;
}
.avatar.avatar-sm {
  width: 60rpx;
  height: 60rpx;
}
.avatar.avatar-md {
  width: 80rpx;
  height: 80rpx;
}
.avatar.avatar-lg {
  width: 120rpx;
  height: 120rpx;
}
.loading {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 32rpx;
}
.loading::after {
  content: "";
  width: 40rpx;
  height: 40rpx;
  border: 4rpx solid #eeeeee;
  border-top-color: #ff6b35;
  border-radius: 50%;
  animation: spin 1s linear infinite;
}
@keyframes spin {
to {
    transform: rotate(360deg);
}
}
.empty {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 48rpx;
  color: #999999;
}
.empty .empty-icon {
  font-size: 120rpx;
  margin-bottom: 24rpx;
  opacity: 0.3;
}
.empty .empty-text {
  font-size: 28rpx;
}
